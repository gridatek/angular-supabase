{
  "name": "angular-supabase-workspace",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "supabase db reset",
    "frontend:install": "npm install --workspace=apps/frontend",
    "frontend:build": "npm run build --workspace=apps/frontend",
    "frontend:e2e": "npm run e2e --workspace=apps/frontend",
    "test:e2e": "npm run db:start && npm run db:seed && npm run frontend:build && npm run frontend:e2e && npm run db:stop"
  },
  "devDependencies": {
    "supabase": "^2.45.5"
  }
}
